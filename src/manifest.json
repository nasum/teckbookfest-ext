{
  "manifest_version": 2,
  "name": "技術書典 売上集計",
  "description": "技術書典の売上を集計します",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}